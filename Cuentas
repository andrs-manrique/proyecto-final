//Crear esta tabla con datos en SQL para hacer Pruebas:
DROP TABLE IF EXISTS `usuarios`;
CREATE TABLE IF NOT EXISTS `usuarios` (
  `usuario_identificador` int(11) NOT NULL,
  `usuario_nombre` varchar(40) NOT NULL,
  `usuario_apellido` varchar(40) NOT NULL,
  `usuario_correo` varchar(60) NOT NULL,
  `usuario_contrasena` varchar(50) NOT NULL,
  `usuario_permisos` varchar(30) NOT NULL,
  PRIMARY KEY (`usuario_identificador`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

--
-- Volcado de datos para la tabla `usuarios`
--

